<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/cardview"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:elevation="5dp"
    app:cardCornerRadius="20dp"
    android:layout_margin="9dp">


    <ImageView
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:background="@drawable/mask"
        android:layout_marginTop="100dp" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/star1"
        android:layout_marginTop="40dp"
        android:layout_marginStart="230dp"/>

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/star2"
        android:layout_marginTop="50dp"
        android:layout_marginStart="240dp"/>


    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/star3"
        android:layout_marginTop="60dp"
        android:layout_marginStart="250dp"/>

    <ImageView
        android:layout_width="130dp"
        android:layout_height="120dp"
        android:background="@drawable/bhagt"
        android:layout_marginTop="140dp"
        android:layout_marginStart="30dp"/>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="280dp"
        android:background="@drawable/green"
        android:layout_marginTop="240dp"/>

    <ImageView
        android:layout_width="70dp"
        android:layout_height="100dp"
        android:background="@drawable/lin"
        android:layout_marginTop="190dp"/>

    <TextView
        android:id="@+id/textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:text="contain"
        android:textSize="20dp"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginTop="280dp"/>


    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/triangle"
        android:layout_marginTop="460dp"
        android:layout_marginStart="40dp"/>

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/triangle3"
        android:layout_marginTop="470dp"
        android:layout_marginStart="50dp"/>


    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/tringale2"
        android:layout_marginTop="482dp"
        android:layout_marginStart="60dp"/>




</FrameLayout>
